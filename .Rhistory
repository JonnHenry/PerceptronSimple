valoresAdaline=function(w,dataFrameOp,factorAprendizaje,numeroVeces){
cont<-0
col<-0
sumaError<-0
errorTotal<-c()
suma<-c()
while(cont<numeroVeces){
valor<-valoresY(w,dataFrameOp)
for (l in 1:7) {
error<-funcionError(dataFrameOp[[4]][[l]],valor[[l]])
sumaError<-sumaError+error
if(error!=0) {
for (j in 1:length(w)){
w[[j]]<-w[[j]]+factorAprendizaje*(dataFrameOp[[ncol(dataFrameOp)]][[l]]-valor[[l]])*dataFrameOp[[j]][[l]]
suma<-w[[j]]+suma
}
# w1<-w1+factorAprendizaje*(dataFrameOp[[4]][[l]]-valor[[l]])*dataFrameOp[[1]][[l]]
# w2<-w2+factorAprendizaje*(dataFrameOp[[4]][[l]]-valor[[l]])*dataFrameOp[[2]][[l]]
# w3<-w3+factorAprendizaje*(dataFrameOp[[4]][[l]]-valor[[l]])*dataFrameOp[[3]][[l]]
valor<-valoresY(w,dataFrameOp)
}
}
errorTotal<-append(errorTotal,sumaError)
cont<-cont+1
sumaError<-0
if (funcionError(dataFrameOp[[4]][[7]],valor[[7]])==0){
break
}
}
errorTotal<-0.5*(errorTotal)
retorno<-list()
retorno[["valoresW"]]<-w
retorno[["error"]]<-errorTotal
return(retorno)
}
dataFrameValores<-data.frame(x1=c(0,0,0,1,1,1,1),x2=c(0,1,1,0,0,1,1),x3=c(1,0,1,0,1,0,1),d=c(1,2,3,4,5,6,7))
resultados<-valoresAdaline(list(0.84,0.39,0.78),dataFrameValores,0.01,100)
